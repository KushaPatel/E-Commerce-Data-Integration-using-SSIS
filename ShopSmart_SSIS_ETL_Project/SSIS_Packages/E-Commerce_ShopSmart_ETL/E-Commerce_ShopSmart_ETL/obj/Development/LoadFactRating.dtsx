      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{CC9230D9-1A5C-4FE4-82FF-5F8B1FD0E271}"
          SQLTask:SqlStatementSource="MERGE INTO FactCustomerRatings AS Target&#xA;USING StgFactCustomerRatings AS Source&#xA;    ON Target.customerproductrating_id = Source.customerproductrating_id&#xA;WHEN MATCHED AND (&#xA;        (Target.customer_id &lt;&gt; Source.customer_id OR (Target.customer_id IS NULL AND Source.customer_id IS NOT NULL) OR (Target.customer_id IS NOT NULL AND Source.customer_id IS NULL)) OR&#xA;        (Target.product_id &lt;&gt; Source.product_id OR (Target.product_id IS NULL AND Source.product_id IS NOT NULL) OR (Target.product_id IS NOT NULL AND Source.product_id IS NULL)) OR&#xA;        (Target.ratings &lt;&gt; Source.ratings) OR&#xA;        (Target.review &lt;&gt; Source.review OR (Target.review IS NULL AND Source.review IS NOT NULL) OR (Target.review IS NOT NULL AND Source.review IS NULL)) OR&#xA;        (Target.sentiment &lt;&gt; Source.sentiment OR (Target.sentiment IS NULL AND Source.sentiment IS NOT NULL) OR (Target.sentiment IS NOT NULL AND Source.sentiment IS NULL))&#xA;    )&#xA;    THEN UPDATE SET&#xA;        Target.customer_id = Source.customer_id,&#xA;        Target.product_id = Source.product_id,&#xA;        Target.ratings = Source.ratings,&#xA;        Target.review = Source.review,&#xA;        Target.sentiment = Source.sentiment&#xA;WHEN NOT MATCHED BY TARGET&#xA;    THEN INSERT (customerproductrating_id, customer_id, product_id, ratings, review, sentiment)&#xA;         VALUES (Source.customerproductrating_id, Source.customer_id, Source.product_id, Source.ratings, Source.review, Source.sentiment)&#xA;OUTPUT&#xA;    $action AS ActionType,&#xA;    inserted.customerproductrating_id AS CustomerProductRatingID,&#xA;    GETDATE() AS ActionDateTime,&#xA;    SYSTEM_USER AS PerformedBy,&#xA;&#xA;    deleted.customer_id AS Old_customer_id,&#xA;    inserted.customer_id AS New_customer_id,&#xA;&#xA;    deleted.product_id AS Old_product_id,&#xA;    inserted.product_id AS New_product_id,&#xA;&#xA;    deleted.ratings AS Old_ratings,&#xA;    inserted.ratings AS New_ratings,&#xA;&#xA;    deleted.review AS Old_review,&#xA;    inserted.review AS New_review,&#xA;&#xA;    deleted.sentiment AS Old_sentiment,&#xA;    inserted.sentiment AS New_sentiment&#xA;INTO CustomerRatingsAuditLog (&#xA;    ActionType,&#xA;    CustomerProductRatingID,&#xA;    ActionDateTime,&#xA;    PerformedBy,&#xA;&#xA;    Old_customer_id,&#xA;    New_customer_id,&#xA;&#xA;    Old_product_id,&#xA;    New_product_id,&#xA;&#xA;    Old_ratings,&#xA;    New_ratings,&#xA;&#xA;    Old_review,&#xA;    New_review,&#xA;&#xA;    Old_sentiment,&#xA;    New_sentiment&#xA;);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
