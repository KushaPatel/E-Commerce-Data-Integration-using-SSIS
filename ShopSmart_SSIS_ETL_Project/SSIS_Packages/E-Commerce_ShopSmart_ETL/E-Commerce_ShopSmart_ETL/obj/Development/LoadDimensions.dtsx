      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{E5B3C05B-6C3C-4A3F-ADE5-DC14A419A3B2}"
          SQLTask:SqlStatementSource="MERGE INTO DimCategory AS Target&#xA;USING StgCategory AS Source&#xA;    ON Target.category_id = Source.category_id&#xA;WHEN MATCHED AND (&#xA;        (Target.category_name &lt;&gt; Source.category_name OR (Target.category_name IS NULL AND Source.category_name IS NOT NULL) OR (Target.category_name IS NOT NULL AND Source.category_name IS NULL))&#xA;    )&#xA;    THEN UPDATE SET&#xA;        Target.category_name = Source.category_name&#xA;WHEN NOT MATCHED BY TARGET&#xA;    THEN INSERT (category_id, category_name)&#xA;         VALUES (Source.category_id, Source.category_name)&#xA;OUTPUT&#xA;    $action AS ActionType,&#xA;    inserted.category_id AS CategoryID,&#xA;    GETDATE() AS ActionDateTime,&#xA;    SYSTEM_USER AS PerformedBy,&#xA;    deleted.category_name AS Old_category_name,&#xA;    inserted.category_name AS New_category_name&#xA;INTO CategoryAuditLog (&#xA;    ActionType,&#xA;    CategoryID,&#xA;    ActionDateTime,&#xA;    PerformedBy,&#xA;    Old_category_name,&#xA;    New_category_name&#xA;);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
