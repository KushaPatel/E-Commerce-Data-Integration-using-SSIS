      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{D5A71211-C1EB-48E0-8D43-E60DB44216CD}"
          SQLTask:SqlStatementSource="MERGE INTO FactCampaignSubcategory AS Target&#xA;USING StgFactCampaignSubcategory AS Source&#xA;    ON Target.campaign_product_subcategory_id = Source.campaign_product_subcategory_id&#xA;WHEN MATCHED AND (&#xA;        (Target.campaign_id &lt;&gt; Source.campaign_id OR (Target.campaign_id IS NULL AND Source.campaign_id IS NOT NULL) OR (Target.campaign_id IS NOT NULL AND Source.campaign_id IS NULL)) OR&#xA;        (Target.subcategory_id &lt;&gt; Source.subcategory_id OR (Target.subcategory_id IS NULL AND Source.subcategory_id IS NOT NULL) OR (Target.subcategory_id IS NOT NULL AND Source.subcategory_id IS NULL)) OR&#xA;        (Target.discount &lt;&gt; Source.discount)&#xA;    )&#xA;    THEN UPDATE SET&#xA;        Target.campaign_id = Source.campaign_id,&#xA;        Target.subcategory_id = Source.subcategory_id,&#xA;        Target.discount = Source.discount&#xA;WHEN NOT MATCHED BY TARGET&#xA;    THEN INSERT (campaign_product_subcategory_id, campaign_id, subcategory_id, discount)&#xA;         VALUES (Source.campaign_product_subcategory_id, Source.campaign_id, Source.subcategory_id, Source.discount)&#xA;OUTPUT&#xA;    $action AS ActionType,&#xA;    inserted.campaign_product_subcategory_id AS CampaignProductSubcategoryID,&#xA;    GETDATE() AS ActionDateTime,&#xA;    SYSTEM_USER AS PerformedBy,&#xA;    deleted.campaign_id AS Old_campaign_id,&#xA;    inserted.campaign_id AS New_campaign_id,&#xA;    deleted.subcategory_id AS Old_subcategory_id,&#xA;    inserted.subcategory_id AS New_subcategory_id,&#xA;    deleted.discount AS Old_discount,&#xA;    inserted.discount AS New_discount&#xA;INTO CampaignSubcategoryAuditLog (&#xA;    ActionType,&#xA;    CampaignProductSubcategoryID,&#xA;    ActionDateTime,&#xA;    PerformedBy,&#xA;    Old_campaign_id,&#xA;    New_campaign_id,&#xA;    Old_subcategory_id,&#xA;    New_subcategory_id,&#xA;    Old_discount,&#xA;    New_discount&#xA;);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
